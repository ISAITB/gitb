{
  "name": "ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --host 0.0.0.0",
    "build": "ng build --progress",
    "build:dev": "ng build --progress --watch",
    "build:prod": "ng build --progress --configuration production --output-hashing none",
    "audit": "better-npm-audit audit --production",
    "licence-report": "license-report --only=prod --output=markdown --fields=licenseType --fields=name --fields=installedVersion --fields=link",
    "licence-report-save": "npm run licence-report > ./dist/THIRD_PARTY_LICENCES.md",
    "licence-check": "license-checker-rseidelsohn --production --depth 0 --excludePrivatePackages --onlyAllow MIT;Apache-2.0 --summary",
    "licence-check-linux": "license-checker-rseidelsohn --production --depth 0 --excludePrivatePackages --onlyAllow MIT\\;Apache-2.0 --summary"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.15",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.15",
    "@angular/compiler": "^19.2.15",
    "@angular/core": "^19.2.15",
    "@angular/forms": "^19.2.15",
    "@angular/platform-browser": "^19.2.15",
    "@angular/platform-browser-dynamic": "^19.2.15",
    "@angular/router": "^19.2.15",
    "@ctrl/ngx-codemirror": "^7.0.0",
    "@fortawesome/fontawesome-free": "6.7.2",
    "@hugerte/hugerte-angular": "^1.0.2",
    "angular2-notifications": "^16.0.1",
    "bootstrap": "5.3.7",
    "codemirror": "5.65.19",
    "file-saver": "^2.0.5",
    "hugerte": "^1.0.9",
    "lodash": "^4.17.21",
    "marked": "^15.0.12",
    "ngx-bootstrap": "19.0.2",
    "ngx-color-picker": "19.0.0",
    "ngx-cookie-service": "19.1.2",
    "ngx-markdown": "^19.1.1",
    "rxjs": "7.8.2",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.16",
    "@angular/cli": "^19.2.16",
    "@angular/compiler-cli": "^19.2.15",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "^5.1.5",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.12.7",
    "better-npm-audit": "^3.11.0",
    "codelyzer": "^6.0.2",
    "license-checker-rseidelsohn": "^4.4.2",
    "license-report": "^6.7.1",
    "ts-node": "~10.9.2",
    "tslib": "^2.7.0",
    "tslint": "~6.1.0",
    "typescript": "^5.7.3"
  }
}
