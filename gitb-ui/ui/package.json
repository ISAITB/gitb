    "@types/jasmine": "^5.1.4",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "better-npm-audit": "^3.7.3",
    "codelyzer": "^6.0.2",
    "ts-node": "~10.9.2",
    "tslib": "^2.5.0",
    "tslint": "~6.1.0",
    "typescript": "^5.4.5"
