{
  "name": "ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --proxy-config src/proxy.conf.js --host 0.0.0.0",
    "build": "ng build --progress --output-path ../app/assets/build",
    "build:dev": "ng build --progress --watch --output-path ../app/assets/build",
    "build:prod": "ng build --progress --configuration production --output-path ../app/assets/build --output-hashing none",
    "audit": "better-npm-audit audit --production"
  },
  "private": true,
