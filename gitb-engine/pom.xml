        </dependency>
		<dependency>
			<groupId>com.gitb</groupId>
			<artifactId>gitb-local-testcase-repository</artifactId>
			<version>1.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.gitb</groupId>
			<artifactId>gitb-messaging</artifactId>
			<version>1.0-SNAPSHOT</version>
