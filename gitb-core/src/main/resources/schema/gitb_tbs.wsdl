            <output
                    wsam:Action="http://www.gitb.com/tbs/v1/TestbedService/getTestCaseDefinitionResponse"
                    message="tns:getTestCaseDefinitionResponse"/>
            <fault message="tns:error" name="error" />
        </operation>
        <operation name="getActorDefinition">
